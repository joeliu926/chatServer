{
  "name": "nodeejs",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "dev_web": "node ./bin/www  8061 dev",
    "dev_ws": "node ./webSocketService/bin/ws.js 8065",
    "dev": "concurrently \"npm run dev_ws\"  \"npm run dev_web\"",
    "sdev_web": "forever start  ./bin/www  8061 dev",
    "sdev_ws": "forever start  ./webSocketService/bin/ws.js 8065",
    "sdev": "concurrently \"npm run sdev_ws\" \"npm run sdev_web\"",
    "test_web": "forever start  ./bin/www  8062 uat",
    "test_ws": "forever start  ./webSocketService/bin/ws.js 8066",
    "test": "concurrently \"npm run test_ws\" \"npm run test_web\"",
    "dist_web": "forever start  ./bin/www  8063 prd",
    "dist_ws": "forever start  ./webSocketService/bin/ws.js 8067",
    "dist": "concurrently \"npm run dist_ws\" \"npm run dist_web\"",
    "_COMMENT_": "_LOCALPORT_, _ENVIRONMENT_"
  },
  "dependencies": {
    "body-parser": "^1.18.2",
    "co": "^4.6.0",
    "concurrently": "^3.6.0",
    "cookie-parse": "^0.4.0",
    "cookie-parser": "^1.4.3",
    "crypto": "^1.0.1",
    "debug": "^2.6.9",
    "ejs": "^2.5.9",
    "express": "^4.17.3",
    "express-session": "^1.15.6",
    "formidable": "^1.2.1",
    "log4js": "^2.5.3",
    "mongoose": "^5.1.6",
    "morgan": "^1.9.0",
    "multiparty": "^4.1.3",
    "mysql": "^2.15.0",
    "mysql2": "^1.5.3",
    "node-schedule": "^1.3.0",
    "node-uuid": "^1.4.8",
    "q": "^1.5.1",
    "querystring": "^0.2.0",
    "request": "^2.87.0",
    "sequelize": "^4.37.6",
    "serve-favicon": "^2.4.5",
    "util": "^0.11.0",
    "uuid": "^3.2.1",
    "ws": "^5.2.1"
  }
}
